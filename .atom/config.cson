"*":
  "atom-runner": {}
  "autocomplete-plus":
    backspaceTriggersAutocomplete: true
    confirmCompletion: "tab always, enter when suggestion explicitly selected"
    strictMatching: true
  core:
    telemetryConsent: "no"
    themes: [
      "one-dark-ui"
      "Warm-Dark-syntax"
    ]
  editor:
    fontSize: 15
    scrollPastEnd: true
  "exception-reporting":
    userId: "b783c150-2d73-4b36-b72a-a096a2755a30"
  minimap:
    absoluteMode: true
  welcome:
    showOnStartup: false
